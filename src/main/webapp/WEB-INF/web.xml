<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<display-name>HomeServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlets.user.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PetDetailServlet</servlet-name>
  	<display-name>PetDetailServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlets.user.PetDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/HomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PetDetailServlet</servlet-name>
  	<url-pattern>/PetDetailServlet</url-pattern>
  </servlet-mapping>
</web-app>
